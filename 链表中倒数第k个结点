//题目描述
//输入一个链表，输出该链表中倒数第k个结点。

/*
public class ListNode {
    int val;
    ListNode next = null;

    ListNode(int val) {
        this.val = val;
    }
}*/
public class Solution {
    public ListNode FindKthToTail(ListNode head,int k) {
        if(head == null)
            return null;
        ListNode fast = head;
        ListNode low = head;
        while(k-- > 0) {
            fast = fast.next;
            if(fast == null && k != 0)
                return null;
        }
        while(fast != null) {
            fast = fast.next;
            low = low.next;
        }
        return low;
    }
}
